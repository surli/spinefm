<?xml version="1.0" encoding="ASCII"?>
<msplm:MultipleSoftwareProductLine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:msplm="msplm.spinefm.unice.fr"
    xsi:schemaLocation="msplm.spinefm.unice.fr ../../../spinefm-core/model/MetamodelSpineFM.ecore#//MSPLModel"
    id="yourcast">
  <domainElements
      belongs_to="//@associations.0"
      id="source">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.0 //@associations.1"
      id="renderer">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.1 //@associations.2 //@associations.3"
      id="zone">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.3"
      id="behaviour">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.2"
      id="layout">
    <MultiplicityElement
        lowerBound="1"
        upperBound="1"/>
  </domainElements>
  <associations
      id="AssoRendererSource">
    <extremity
        apply_on="//@domainElements.1"
        id="assoRSRenderer">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </extremity>
    <extremity
        apply_on="//@domainElements.0"
        id="assoRSSource">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </extremity>
  </associations>
  <associations
      id="AssoRendererZone">
    <extremity
        apply_on="//@domainElements.1"
        id="assoRZRenderer">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="-1"/>
    </extremity>
    <extremity
        apply_on="//@domainElements.2"
        id="assoRZZone">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </extremity>
  </associations>
  <associations
      id="AssoLayoutZone">
    <extremity
        apply_on="//@domainElements.4"
        id="assoLZLayout">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </extremity>
    <extremity
        apply_on="//@domainElements.2"
        id="assoLZZone">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="-1"/>
    </extremity>
  </associations>
  <associations
      id="AssoBehaviourZone">
    <extremity
        apply_on="//@domainElements.3"
        id="assoBZBehaviour">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </extremity>
    <extremity
        apply_on="//@domainElements.2"
        id="assoBZZone">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </extremity>
  </associations>
</msplm:MultipleSoftwareProductLine>
