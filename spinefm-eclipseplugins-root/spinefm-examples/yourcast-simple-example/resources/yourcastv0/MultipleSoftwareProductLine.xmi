<?xml version="1.0" encoding="ASCII"?>
<msplm:MultipleSoftwareProductLine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:msplm="msplm.spinefm.unice.fr"
    xsi:schemaLocation="msplm.spinefm.unice.fr ../MetamodelSpineFM.ecore#//MSPLModel">
  <domainElements
      belongs_to="//@associations.0"
      id="source">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.0 //@associations.1"
      id="renderer">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.1 //@associations.2 //@associations.3"
      id="zone">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.3"
      id="layout">
    <MultiplicityElement
        lowerBound="1"
        upperBound="1"/>
  </domainElements>
  <domainElements
      belongs_to="//@associations.2"
      id="behaviour">
    <MultiplicityElement
        lowerBound="1"
        upperBound="-1"/>
  </domainElements>
  <associations
      id="AssoSourceRenderer">
    <source
        apply_on="//@domainElements.0">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </source>
    <target
        apply_on="//@domainElements.1">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </target>
  </associations>
  <associations
      id="AssoRendererZone">
    <source
        apply_on="//@domainElements.1">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="-1"/>
    </source>
    <target
        apply_on="//@domainElements.2">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </target>
  </associations>
  <associations
      id="AssoBehaviourZone">
    <source
        apply_on="//@domainElements.4">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </source>
    <target
        apply_on="//@domainElements.2">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </target>
  </associations>
  <associations
      id="AssoLayoutZone">
    <source
        apply_on="//@domainElements.3">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="1"/>
    </source>
    <target
        apply_on="//@domainElements.2">
      <LinkMultiplicity
          lowerBound="1"
          upperBound="-1"/>
    </target>
  </associations>
</msplm:MultipleSoftwareProductLine>
