fm_renderer = FM(Renderer: TypeInfo [Criteria] Elements Output Discretization; 
				TypeInfo: (Tweet|Calendar|PictureAlbum|Forecast|Ephemeride|Announces); 
				Criteria: [Clustering]; 
				Elements: [Title] Content [Logo]; 
				Output: (Block|Line); 
				Discretization: Spatial Temporal; 
				Clustering: (ByHour|ByMonth|ByDay|BySize); 
				Content: (Graphic|Text)+; 
				Spatial: (FixedNumber|All|Unique); 
				Temporal: (FixedTime|FlexibleTime); 
				Graphic: (Thumbnails|FullPicture); 
				FixedNumber: [TenInformations]; 
				(Ephemeride -> !FullPicture);
				(Ephemeride -> Text);
				(FullPicture -> !Announces);
				(ByDay -> Text);
				(Forecast -> Title);
				(Line -> Tweet);
				(Ephemeride -> FixedTime);
				(BySize -> PictureAlbum);
				(Title -> Block);
				(All -> !FullPicture);
				(Announces -> Unique);
				(Unique -> !Tweet);
				(Clustering -> Title);
				(Logo -> !Unique);
				(Calendar -> All);
				(All -> Text);
				(Calendar -> FlexibleTime);
				(ByMonth -> Calendar);
				(TenInformations -> FixedTime);
				(Forecast -> Thumbnails);
				(Announces -> Text);
				(Graphic -> !ByMonth);
				(FullPicture -> Title);
				(Forecast -> FixedTime);
				(Logo -> Tweet);
				(ByDay -> !Tweet);
				(All -> !PictureAlbum);
				(TenInformations -> Line);
				(Thumbnails -> !TenInformations);
				(Unique -> !Clustering);
				(Text -> !PictureAlbum);
				(TenInformations -> Logo);
				(Unique -> !Line);
				(Graphic -> !Calendar);
				(BySize -> FlexibleTime);
				(Ephemeride -> Title);
				(Tweet -> Text);
				(ByHour -> Calendar);
				(Ephemeride -> Unique);
				(Calendar -> Title);
				(BySize -> FixedNumber);
				(BySize -> !Text);
				(PictureAlbum -> FullPicture);
				(Thumbnails -> Text);
				(Graphic -> !ByHour);
				(Ephemeride -> !Thumbnails);)